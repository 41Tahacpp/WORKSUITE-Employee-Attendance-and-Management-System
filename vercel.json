{
    "builds": [
      {
        "src": "Frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "Frontend/dist"
        }
      },
      {
        "src": "Server/package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "Server/$1"
      },
      {
        "src": "/(.*)",
        "dest": "Frontend/dist/$1"
      }
    ]
  }
  